<p style="color: green"><%= notice %></p>

<span><h1 class="text-dark text-center ">Vehicles</h1></span>
<div class="btn btn-danger ">
<%= link_to "Add new vehicle", new_vehicle_path, class:"text-dark", style:"text-decoration:none;"%>
</div>


<div id="vehicles" class="container">
   <div class="row ">
  <% @vehicles.each do |vehicle| %>
     
     <div class="card col-2 bg-success mx-2 my-2">
    <%= render vehicle %>
    <hr>
    <p>
      <%= link_to "Show this vehicle", vehicle, class:"text-danger" %>
    </p>
    </div>
  <% end %>
   </div>
</div>


